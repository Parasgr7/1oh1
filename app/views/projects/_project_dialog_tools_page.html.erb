<!-- Modal -->
<div class="modal fade" id="projectDialogToolsPage" tabindex="-1" role="dialog" aria-labelledby="projectDialogToolsPage" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content p-4 large-size">
      <%= form_tag(project_edit_details_path(project),  :method => :put,remote: true,:id => 'edit_project_details' ) do |f| %>
        <div class="modal-body">
            <%= render 'project_page_tools' %>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-link" data-dismiss="modal">Cancel</button>
            <%= submit_tag("Save",:id=>"submit_dialog", :class=>'btn btn-success',data: { disable_with: "Saving..." })%>
        </div>
      <%end%>
    </div>
  </div>
</div>
