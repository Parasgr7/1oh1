<!-- Modal -->
<div class="modal fade" id="projectDialogSupportPage" tabindex="-1" role="dialog" aria-labelledby="projectDialogSupportPageLabel" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content p-4 medium-plus-size">
      <%= form_tag(project_edit_collaborators_path(project),  :method => :put,remote: true,:id => 'edit_project_support' ) do |f| %>
      <div class="modal-body">
        <%= render partial:'project_page_support',locals:{ isNewProject: false,collaborators: project.colab_id }%>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-link" data-dismiss="modal">Cancel</button>
        <%= submit_tag("Save",:id=>"submit_dialog_collab", :class=>'btn btn-success',data: { disable_with: "Saving..." })%>
      </div>
      <%end%>
    </div>
  </div>
</div>
