<%= render 'shared_components' %>

<div class="container">
	<div class="row">
		<div class="col-12 col-lg-12">
			<div id="filter_results_all">
			</div>
			<div id="popular_results_categories_all">
				<% if request.parameters[:search] %>
		      <%= render partial:'shared/search_results',locals:{results: @results}%>
		    <%else%>
					<%= render partial: 'categories/filter_results', locals: { explores: @results_explores,guides: @results_guides,filter:"Popular",category: @category}%>
				<%end%>
			</div>

		</div>
	</div>
</div>

<%= render 'landing/footer' %>
