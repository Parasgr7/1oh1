<div id="filterrific_results_explores">
    <h4 class="my-5 font-weight-bold c-black"><%=filter.camelize%> Explorers for <%= category.name.camelize %></h4>
    <div class="row">
        <% if explores.blank? %>
        <h6 class="mt-3 c-black col">
        	No Explores available
      	</h6>
        <%else%>
          <%= render partial: 'shared/user_card.html', collection: explores, as: :user,locals:{ class_name: "popularRolItem",type:"Explorer"} %>.
        <%end%>
    </div>


</div>
