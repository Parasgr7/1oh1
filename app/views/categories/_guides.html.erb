<div id="filterrific_results_guides">
    <h4 class="my-5 font-weight-bold c-black"><%=filter.camelize%> Guides for <%=category.name.camelize%></h4>
    <div class="row">
      <% if guides.blank? %>
      	<h6 class="mt-3 c-black col">
        	No Guides available
      	</h6>
      <%else%>
        <%= render partial: 'shared/user_card.html', collection: guides, as: :user,locals:{ class_name: "popularRolItem",type:"Guide"} %>
      <%end%>
    </div>
</div>
