<div class="modal fade" id="createProjectsModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered large-size" role="document">
    <div class="modal-content px-3">
      <%= form_tag( create_project_builder_path, :method => :post,remote: true ,:id => 'create_project' ) do |f| %>
        <div class="modal-header">
          <h5 class="modal-title">Create Project</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body" style="overflow-x: hidden;">
          <div>
            <%= render 'projects/project_page_main' %>
          </div>
          <div class="mt-4 create-project-profile-project-page-support-wrap">
            <%= render 'projects/project_page_support', isNewProject: true %>
          </div>
          <div class="mt-4">
            <%= render 'projects/project_page_tools', isNewProject: true %>
          </div>
        </div>
        <div class="modal-footer">
           <div class="celr" id="gridbtnSave">
               <div class="cel btnSave">
                <%= submit_tag("Create Project",:id=>"submit_create_project", :class=>'btn btn-success',data: { disable_with: "Uploading..." })%>
              </div>
           </div>
        </div>

      <% end %>

    </div>
  </div>
</div>
